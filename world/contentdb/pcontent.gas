[formulas]
{
	// these blocks define the "infinite power" weapons - these will always be
	// able to go up to any power level by feedback into the modifiers.

	[infinite_melee_weapons]
		{
			* = sword;
			* = axe;
			* = staff;
		}

	[infinite_ranged_weapons]
		{
			* = bow;
		}
}

//////////////////////////////////////////////////////////////////////////////

[macros]
{
	supersword = sword/500;
}

