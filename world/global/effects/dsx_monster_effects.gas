			sfx target #PEEK target;
			sfx attach_point #PEEK @shield_bone target;
			sfx start #POP; 

			sfx create curve #SOURCE
				"delay(0.6)color0(.6,.6,1)ts(2.7)";
			sfx target #PEEK target;
			sfx attach_point #PEEK @weapon_bone target;
			sfx start #POP;

			sfx create curve #SOURCE
				"delay(0.65)color0(.6,.6,1)ts(2.8)";
			sfx target #PEEK target;
			sfx attach_point #PEEK @shield_bone target;
			sfx start #POP; 

			sfx create curve #SOURCE
				"delay(0.7)color0(.6,.6,1)ts(2.9)";
