[effect_script*]
{
	name	=dsx_giant_spider_foot_spark_1;

	script	=[[
			camerashake camera_stomp s<frequency=2&magnitude_x=0.0&magnitude_y=0.1&magnitude_z=0.0&duration=.05&epicenter=#SOURCE_GOID>;
			
			sound play s_e_smash_rockbeast at #SOURCE_POSITION dist 5 20;
			
			sfx create explosion #TARGET
			"scale(.5)texture(b_sfx_rock_single_01)ivel(0,2,0)omni_dir()
			color0(.2,.2,.2)color1(0,0,0)dark()dur(4)count(12)collide()";
			
			
			sfx attach_point #PEEK L1_foot target;
			sfx start #POP;
			
			sfx create orbiter #TARGET 
				"dur(.2)tin(0)tout(.2)scale(.5)color0(.4,.25,.07)iphi(0)
				radius(0)";
			sfx attach_point #PEEK L1_foot target;
			sfx start #POP;

			sfx create fire #TARGET 
				"scale(1)dur(.2)dark()grey_tex()color0(.3,.3,.3)color1(0,0,0)
				count(20)alphafade(1)flamesize(4)max_radius(6)instant()
				velocity(0,2,0)accel(0,0,0)";
			sfx attach_point #PEEK L1_foot target;
			sfx start #POP;
			
			]];
}

